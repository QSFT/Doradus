<TEST>

<DEFINE name="schema.xml">
<application name="Application">
    <key>ApplicationKey</key>
    <options>
	<option name="AutoTables">false</option>
	<option name="StorageService">SpiderService</option>
    </options>
    <tables>
        <table name="Table">
            <fields>
                <field name="IsMajor" type="BOOLEAN"/>
                <field name="SendDate" type="TIMESTAMP"/>
                <field name="Location">
	            <fields>
			<field name="Department" type="Text" />
			<field name="Office" type="Text" />
			<field name="Rooms" type="Text" collection="true" />
			<field name="Size" type="INTEGER" />
        	    </fields>
                </field>
                <field name="Manager">
	            <fields>
			<field name="FirstName" type="Text" />
			<field name="LastName" type="Text" />
        	    </fields>
                </field>
            </fields>
        </table>
    </tables>
</application>
</DEFINE>

