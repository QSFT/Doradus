<TEST>

<DEFINE name="schema.xml">
<application name="Application">
    <key>ApplicationKey</key>
    <options>
	<option name="StorageService">OLAPService</option>
    </options>
    <tables>
	<table name="Table">
	    <fields>
		<field name="MV" type="TEXT" collection="true"/>
		<field name="Text" type="TEXT"/>
	    </fields>
	</table>
    </tables>
</application>
</DEFINE>

<DEFINE name="data.xml">
      <batch>
        <docs>
          <doc _table="Table" _ID="1001" >
            <field name="Text">str1</field>
          </doc>
          <doc _table="Table" _ID="1002" >
            <field name="MV">
		<add>
		    <value>Confidential</value>
		</add>
	    </field>
            <field name="Text">str2</field>
          </doc>
          <doc _table="Table" _ID="1003" >
            <field name="MV">
		<add>
		    <value>Secret</value>
		    <value>Confidential</value>
		</add>
	    </field>
            <field name="Text">str3</field>
          </doc>
          <doc _table="Table" _ID="1004" >
            <field name="MV">
		<add>
		    <value>Secret</value>
		    <value>Top Secret</value>
		    <value>Public</value>
		</add>
	    </field>
            <field name="Text">str4</field>
          </doc>
          <doc _table="Table" _ID="1005" >
            <field name="MV">
		<add>
		    <value>Top Confidential</value>
		    <value>Top Secret</value>
		</add>
	    </field>
            <field name="Text">str5</field>
          </doc>
          <doc _table="Table" _ID="1006" >
            <field name="MV">
		<add>
		    <value>Secret</value>
		</add>
	    </field>
            <field name="Text">str6</field>
          </doc>
          <doc _table="Table" _ID="1007" >
            <field name="MV">
		<add>
		    <value>Extra Secret</value>
		</add>
	    </field>
            <field name="Text">str7</field>
          </doc>
        </docs>
      </batch>
</DEFINE>

<JSON-FROM-XML name ="schema.json" value="${schema.xml}"/>
<JSON-FROM-XML name ="data.json" value="${data.xml}"/>

</TEST>